          <Font name="Segoe UI" size="24" style="1"/>
        </Property>
        <Property name="text" type="java.lang.String" value="Forces arm&#xe9;es &#xe9;gyptiennes : bouclier et partenaire du d&#xe9;veloppement"/>
      </Properties>
    </Component>
    <Container class="javax.swing.JScrollPane" name="jScrollPane2">
